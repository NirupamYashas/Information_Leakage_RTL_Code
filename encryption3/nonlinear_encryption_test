 .timescale -9 -12;
TD_tb_nonlinear_encryption_module.perform_test ;
    %vpi_call 2 99 "$display", "Time: %t, Input: %b, Key: %b, Output: %b, Expected: %b", $time, v0x13d72a960_0, v0x13d72ab70_0, v0x13d72aa10_0, v0x13d72aac0_0 {0 0 0};
    %load/vec4 v0x13d72aa10_0;
    %load/vec4 v0x13d72aac0_0;
    %cmp/ne;
    %jmp/0xz  T_1.4, 6;
    %vpi_call 2 102 "$display", "Error: Output does not match expected value." {0 0 0};
    %vpi_call 2 103 "$finish" {0 0 0};
T_1.4 ;
    %end;
