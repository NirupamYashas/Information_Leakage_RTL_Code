S_0x13d70e5b0 .scope function.vec4.s8, "calculate_expected_output" "calculate_expected_output" 2 18, 2 18 0, S_0x13d70e440;
 .timescale -9 -12;
; Variable calculate_expected_output is vec4 return value of scope S_0x13d70e5b0
v0x13d729d80_0 .var "data", 7 0;
v0x13d729e20_0 .var/i "i", 31 0;
v0x13d729ed0_0 .var "intermediate", 7 0;
v0x13d729f80_0 .var "k", 7 0;
TD_tb_nonlinear_encryption_module.calculate_expected_output ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x13d729e20_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x13d729e20_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x13d729d80_0;
    %load/vec4 v0x13d729e20_0;
    %part/s 1;
    %load/vec4 v0x13d729f80_0;
    %load/vec4 v0x13d729e20_0;
    %part/s 1;
    %xor;
    %ix/getv/s 4, v0x13d729e20_0;
    %store/vec4 v0x13d729ed0_0, 4, 1;
    %load/vec4 v0x13d729e20_0;
    %addi 1, 0, 32;
    %store/vec4 v0x13d729e20_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x13d729e20_0, 0, 32;
T_0.2 ;
    %load/vec4 v0x13d729e20_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_0.3, 5;
    %load/vec4 v0x13d729ed0_0;
    %load/vec4 v0x13d729e20_0;
    %part/s 1;
    %pad/u 32;
    %load/vec4 v0x13d729ed0_0;
    %load/vec4 v0x13d729e20_0;
    %addi 1, 0, 32;
    %pushi/vec4 8, 0, 32;
    %mod/s;
    %part/s 1;
    %pad/u 32;
    %load/vec4 v0x13d729f80_0;
    %load/vec4 v0x13d729e20_0;
    %part/s 1;
    %pad/u 32;
    %and;
    %add;
    %pushi/vec4 2, 0, 32;
    %mod;
    %pad/u 1;
    %ix/getv/s 4, v0x13d729e20_0;
    %ret/vec4 0, 4, 1; Assign to calculate_expected_output (store_vec4_to_lval)
    %load/vec4 v0x13d729e20_0;
    %addi 1, 0, 32;
    %store/vec4 v0x13d729e20_0, 0, 32;
    %jmp T_0.2;
T_0.3 ;
    %end;
