TD_tb_encryption_module_with_timing_check.calculate_expected_output ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x11d627140_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x11d627140_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x11d6271f0_0;
    %load/vec4 v0x11d627140_0;
    %part/s 1;
    %flag_set/vec4 8;
    %jmp/0 T_0.2, 8;
    %pushi/vec4 0, 0, 1;
    %jmp/1 T_0.3, 8;
T_0.2 ; End of true expr.
    %load/vec4 v0x11d6270a0_0;
    %load/vec4 v0x11d627140_0;
    %part/s 1;
    %jmp/0 T_0.3, 8;
 ; End of false expr.
    %blend;
T_0.3;
    %ix/getv/s 4, v0x11d627140_0;
    %ret/vec4 0, 4, 1; Assign to calculate_expected_output (store_vec4_to_lval)
    %load/vec4 v0x11d627140_0;
    %addi 1, 0, 32;
    %store/vec4 v0x11d627140_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
